{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "imperial_calendar": ["../imperial_calendar/src/index.ts"],
      "imperial_calendar/*": ["../imperial_calendar/src/*"],
      "calendar_svg": ["../calendar_svg/src/index.ts"],
      "calendar_svg/*": ["../calendar_svg/src/*"]
    },
    "jsx": "react-jsx",
    "module": "ESNext",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "skipLibCheck": true,
    "types": ["vite/client"]
  },
  "include": ["src"]
}
